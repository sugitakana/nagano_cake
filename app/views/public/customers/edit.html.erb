<p>会員情報編集</p>

<%= form_with model: @customer, url: customers_mypage_path do |f| %>
  <table>
    <tr>
      <th><%= f.label :氏名 %></th>
      <td><%= f.text_field :last_name %> <%= f.text_field :first_name %></td>
    </tr>
    <tr>
      <th><%= f.label :フリガナ %></th>
      <td><%= f.text_field :last_name_kana %><%= f.text_field :first_name_kana %></td>
    </tr>
    <tr>
      <th><%= f.label :郵便番号%></th>
      <td><%= f.text_field :postal_code %></td>
    </tr>
    <tr>
      <th><%= f.label :住所 %></th>
      <td><%= f.text_field :address %></td>
    </tr>
    <tr>
      <th><%= f.label :電話番号 %></th>
      <td><%= f.text_field :telephone_number %></td>
    </tr>
    <tr>
      <th><%= f.label :メールアドレス %></th>
      <td><%= f.text_field :email %></td>
    </tr>
  </table>

  <%= f.submit "編集内容を保存" %>

<% end %>

<%#= link_to "退会する", %>